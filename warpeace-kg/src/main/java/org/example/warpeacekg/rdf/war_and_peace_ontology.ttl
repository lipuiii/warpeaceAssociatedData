<http://wrrc.org/wap#russianName>
        rdf:type     owl:DatatypeProperty;
        rdfs:domain  <http://wrrc.org/wap#Person>;
        rdfs:label   "俄文名"@zh , "Russian name"@en;
        rdfs:range   xsd:string .
