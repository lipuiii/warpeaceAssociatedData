        rdfs:label   "受雇于"@zh , "employed by"@en;
        rdfs:range   <http://wrrc.org/wap#Person> .

<http://wrrc.org/wap#serves>
        rdf:type       owl:ObjectProperty;
        rdfs:domain    <http://wrrc.org/wap#Person>;
        rdfs:label     "服务于"@zh , "serves"@en;
        rdfs:range     <http://wrrc.org/wap#Person>;
